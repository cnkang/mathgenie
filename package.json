{
  "name": "mathgenie",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "sideEffects": [
    "*.css",
    "./src/index.tsx",
    "./src/reportWebVitals.ts",
    "./src/utils/serviceWorker.ts"
  ],
  "dependencies": {
    "@vercel/speed-insights": "^1.2.0",
    "jspdf": "^3.0.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "web-vitals": "^5.1.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsx scripts/optimize-build.ts",
    "build:fast": "vite build",
    "preview": "vite preview",
    "test": "npm-run-all test:unit",
    "test:all": "npm-run-all test:unit test:e2e:quick",
    "test:unit": "vitest --run --coverage",
    "prepare": "husky",
    "coverage": "vitest --coverage",
    "verify": "node scripts/verify-pnpm.js",
    "clean": "rm -rf node_modules pnpm-lock.yaml dist coverage",
    "reinstall": "pnpm clean && pnpm install",
    "lint": "eslint src --ext .ts,.tsx --fix --max-warnings 0 && prettier --write \"src/**/*.{ts,tsx}\" --config .prettierrc.json",
    "lint:sonar": "tsx scripts/sonar-check.ts",
    "lint:sonar:fix": "tsx scripts/sonar-check.ts --fix",
    "lint:sonar:all": "tsx scripts/sonar-check.ts --warnings",
    "lint:css": "stylelint \"src/**/*.css\" \"public/**/*.css\" --formatter string",
    "lint:css:fix": "stylelint \"src/**/*.css\" \"public/**/*.css\" --fix",
    "lint:html": "html-validate \"index.html\" \"public/**/*.html\" \"src/**/*.html\"",
    "lint:css-html": "tsx scripts/css-html-quality-check.ts",
    "lint:css-html:fix": "tsx scripts/css-html-quality-check.ts --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,json}\" --config .prettierrc.json",
    "type-check": "tsc --noEmit",
    "build:types": "tsc --emitDeclarationOnly",
    "analyze": "pnpm build:fast && npx vite-bundle-analyzer dist/assets/*.js",
    "deps:check": "pnpm --silent outdated --no-color --format table || true",
    "test:watch": "vitest",
    "test:e2e": "playwright test --config=playwright.config.ts",
    "test:e2e:ci": "CI=true NO_COLOR=1 playwright test --config=playwright.config.ts --project=chromium --reporter=html --reporter=json",
    "test:e2e:no-colors": "NO_COLOR=1 playwright test --config=playwright-no-colors.config.ts",
    "test:smoke": "playwright test tests/e2e/basic.spec.ts --config=playwright.config.ts --project=chromium",
    "test:e2e:ui": "playwright test --config=playwright.config.ts --ui",
    "test:e2e:headed": "playwright test --config=playwright.config.ts --headed",
    "test:e2e:debug": "playwright test --config=playwright.config.ts --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:enhanced": "playwright test --config=playwright.e2e.config.ts",
    "test:e2e:error-handling": "playwright test tests/e2e/error-handling.spec.ts",
    "test:e2e:localstorage": "playwright test tests/e2e/localstorage-persistence.spec.ts",
    "test:e2e:presets": "playwright test tests/e2e/presets-functionality.spec.ts",
    "test:e2e:integration": "playwright test tests/e2e/integration.spec.ts",
    "test:e2e:accessibility": "CI=true playwright test tests/e2e/accessibility-unified.spec.ts --config=playwright.config.ts --project=chromium --project=firefox --project=webkit",
    "test:e2e:quick": "./scripts/test-e2e.sh quick",
    "test:e2e:full": "./scripts/test-e2e.sh full",
    "test:mobile": "./scripts/test-e2e.sh mobile all",
    "test:mobile:iphone": "./scripts/test-e2e.sh mobile iphone",
    "test:mobile:iphone16": "./scripts/test-e2e.sh mobile iphone16",
    "test:mobile:iphone15": "./scripts/test-e2e.sh mobile iphone15",
    "test:mobile:ipad": "./scripts/test-e2e.sh mobile ipad",
    "test:mobile:portrait": "./scripts/test-e2e.sh mobile portrait",
    "test:mobile:landscape": "./scripts/test-e2e.sh mobile landscape",
    "test:mobile:latest": "./scripts/test-e2e.sh mobile latest",
    "test:mobile:android": "./scripts/test-e2e.sh mobile android",
    "test:mobile:android:phones": "./scripts/test-e2e.sh mobile android-phones",
    "test:mobile:android:tablets": "./scripts/test-e2e.sh mobile android-tablets",
    "test:mobile:galaxy": "./scripts/test-e2e.sh mobile galaxy",
    "test:mobile:pixel": "./scripts/test-e2e.sh mobile pixel",
    "test:mobile:e2e": "./scripts/test-e2e.sh mobile-e2e all",
    "test:mobile:e2e:iphone": "./scripts/test-e2e.sh mobile-e2e iphone",
    "test:mobile:e2e:ipad": "./scripts/test-e2e.sh mobile-e2e ipad",
    "test:mobile:e2e:android": "./scripts/test-e2e.sh mobile-e2e android",
    "test:mobile:e2e:galaxy": "./scripts/test-e2e.sh mobile-e2e galaxy",
    "test:mobile:e2e:pixel": "./scripts/test-e2e.sh mobile-e2e pixel",
    "playwright:install": "playwright install chromium firefox webkit --with-deps",
    "playwright:install:ci": "playwright install chromium firefox webkit --with-deps",
    "playwright:check": "tsx scripts/check-browsers.ts",
    "playwright:cache:check": "./scripts/playwright-cache-helper.sh check all",
    "playwright:cache:debug": "./scripts/debug-playwright-cache.sh",
    "playwright:cache:clean": "./scripts/playwright-cache-helper.sh clean",
    "pre-commit": "pnpm format && lint-staged",
    "pre-push": "pnpm type-check && pnpm i18n:check && pnpm test:unit && pnpm build",
    "commit": "git-cz",
    "validate": "pnpm lint && pnpm lint:css-html && pnpm type-check && pnpm i18n:check && pnpm test && pnpm build && pnpm playwright:check && pnpm test:e2e",
    "lighthouse": "lhci autorun",
    "lighthouse:ci": "lhci autorun --fatal-assert-errors",
    "lighthouse:collect": "lhci collect",
    "lighthouse:assert": "lhci assert --fatal-assert-errors",
    "lighthouse:upload": "lhci upload",
    "i18n:check": "tsx scripts/check-i18n.ts"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@eslint/js": "^9.35.0",
    "@fullhuman/postcss-purgecss": "^7.0.2",
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.55.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.3.1",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "c8": "^10.1.3",
    "cssnano": "^7.1.1",
    "cssnano-preset-advanced": "^7.0.9",
    "eslint": "^9.35.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-sonarjs": "^3.0.5",
    "globals": "^16.4.0",
    "happy-dom": "^18.0.1",
    "html-validate": "^10.0.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jiti": "^2.5.1",
    "lint-staged": "^16.1.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "stylelint": "^16.24.0",
    "stylelint-config-recess-order": "^7.3.0",
    "stylelint-config-standard": "^39.0.0",
    "terser": "^5.44.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vite": "^7.1.5",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix --max-warnings=0",
      "prettier --write --config .prettierrc.json"
    ],
    "*.{css,scss,less}": [
      "stylelint --fix",
      "prettier --write --config .prettierrc.json"
    ],
    "*.html": [
      "html-validate"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write --config .prettierrc.json"
    ]
  },
  "pnpm": {
    "overrides": {
      "glob": "^11.0.3",
      "rimraf": "^6.0.1",
      "chrome-launcher": "^1.2.0",
      "tmp": "^0.2.5",
      "os-tmpdir": "^2.0.0"
    }
  },
  "packageManager": "pnpm@10.15.1"
}
